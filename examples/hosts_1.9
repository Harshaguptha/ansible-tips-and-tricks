---

[examples:children]
single-box
multi-os

[single-box]
#remote_host ansible_ssh_host=X.X.X.X ansible_ssh_port=22
# Vagrant uses a Virtualbox port forward from the VMs port 22 to host systems port 2222
vagrant-box ansible_ssh_host=127.0.0.1 ansible_ssh_port=2222

[multi-os]
#remote_host ansible_ssh_host=X.X.X.X ansible_ssh_port=22
# Vagrant uses a private network within Virtualbox on the 192.168.100.0/24 network
ubuntu ansible_ssh_host=192.168.100.11 ansible_ssh_port=22
centos ansible_ssh_host=192.168.100.12 ansible_ssh_port=22
