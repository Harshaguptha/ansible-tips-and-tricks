---

[examples:children]
single-box
multi-os

[single-box]
#remote_host ansible_ssh_host=X.X.X.X ansible_ssh_port=22
# Vagrant uses a Virtualbox port forward from the VMs port 22 to host systems port 2222
vagrant-box ansible_ssh_host=127.0.0.1 ansible_ssh_port=2222 ansible_ssh_user=root

[multi-os]
#remote_host ansible_ssh_host=X.X.X.X ansible_ssh_port=22 ansible_ssh_user=root
ubuntu ansible_ssh_host=127.0.0.1 ansible_ssh_port=2223 ansible_ssh_user=root
centos ansible_ssh_host=127.0.0.1 ansible_ssh_port=2224 ansible_ssh_user=root

